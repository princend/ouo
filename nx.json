{
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "affected": {
    "defaultBase": "master"
  },
  "npmScope": "sample",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ]
      }
    }
  },
  "projects": {
    "sample-test": {
      "tags": []
    },
    "sample-test-e2e": {
      "tags": [],
      "implicitDependencies": [
        "sample-test"
      ]
    },
    "ouo-ui": {
      "tags": []
    },
    "ouo-render": {
      "tags": []
    },
    "ouo-cms": {
      "tags": []
    },
    "ouo-lib": {
      "tags": []
    }
  }
}
